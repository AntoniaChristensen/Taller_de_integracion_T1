<section class="section">
  <div class="container">
    <article class="media">
      <figure class="media-left">
        <p class="image is-128x128">
          <%if @serie == "Breaking Bad" %>
            <%= image_tag("https://m.media-amazon.com/images/M/MV5BMjhiMzgxZTctNDc1Ni00OTIxLTlhMTYtZTA3ZWFkODRkNmE2XkEyXkFqcGdeQXVyNzkwMjQ5NzM@._V1_.jpg") %>
          <% else %>
            <%= image_tag("https://encrypted-tbn1.gstatic.com/images?q=tbn:ANd9GcSPGTwuT0Wg5XHC71WlpvmHInG00hkhDd4SiUGNjlQALyvgr6Fh") %>
          <%end %>
        </p>
      </figure>
      <div class="media-content">
        <div class="content">
          <p>
          <h1> <%= @serie %></h1> <h4><%= "Season %d" % params[:id]%></h4>
          <br>
          <h5>Episodes:</h5>
          <% i = 1 %>
          <% @temp.each do |e| %>
            <td><%="%d. " % i %><%= link_to e, episode_path(@episodios[e])%></td> <br>
            <%i+= 1 %>
          <% end %>
          <br>
          </p>
        </div>
      </div>
    </article>
  </div>
</section>
